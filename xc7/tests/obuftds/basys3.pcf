# basys3 100 MHz CLK
set_io clk W5

# rst signal
set_io rst U17

# sw[0:15] correspond with SW0-SW15 on the basys3
set_io sw[0] V17
set_io sw[1] V16
set_io sw[2] W16
set_io sw[3] W17
set_io sw[4] W15
set_io sw[5] V15
set_io sw[6] W14
set_io sw[7] W13

# led[0:15] correspond with LD0-LD15 on the basys3
set_io diff_p U16
set_io diff_n E19
